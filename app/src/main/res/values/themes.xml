<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    <style name="Base.Theme.B07Planetze" parent="Theme.Material3.Light.NoActionBar">
        <!-- Primary colors -->
        <item name="colorPrimary">@color/tealish</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary colors -->
        <item name="colorSecondary">@color/tealish</item>
        <item name="colorOnSecondary">@color/black</item>

        <!-- Background and surface colors -->
        <item name="android:colorBackground">@color/white</item>
        <item name="colorOnBackground">@color/white</item>
        <item name="colorSurface">@color/white</item>
        <item name="colorOnSurface">@color/black</item>

        <!-- Error colors -->
        <item name="colorError">"@color/error_red"</item>
        <item name="colorOnError">@color/white</item>
    </style>

    <style name="Theme.B07Planetze" parent="Base.Theme.B07Planetze" />
</resources>